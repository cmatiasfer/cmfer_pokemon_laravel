                                        },
                                        "pok_base_experience": {
                                            "type": "integer"
                                        },
                                        "type": {
                                            "description": "The response data",
                                            "type": "array",
                                            "items": {}
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
